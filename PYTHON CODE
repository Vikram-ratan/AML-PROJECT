   "source": [
    "# Scale BodyweightKg\n",
    "max_body = openpl['BodyweightKg'].max() \n",
    "min_body = openpl['BodyweightKg'].min() \n",
    "openpl['Bodyweight_scaled'] = (openpl['BodyweightKg']-min_body)/(max_body-min_body)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 119,
